        "electron-builder": "^23.1.0",
        "graphql": "^16.5.0",
        "graphql-tag": "^2.12.6",
        "postcss": "^8",
        "tailwindcss": "^3",
        "vue": "^3.2.13"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@headlessui/vue": "^1.6.5",
